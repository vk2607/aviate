<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.piedpipergeeks.aviate">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateEvent" />
        <activity android:name=".Settings"></activity>

        <activity android:name=".EventsList"
             android:label="@string/title_activity_events_list"/>
             <!--android:parentActivityName=".ClubDetails"/>-->

        <activity
            android:name=".ClubDetails"
            android:label="@string/title_activity_club_details"
            android:parentActivityName=".MessageActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".PickClubActivity"
            android:label="Select club"
            android:parentActivityName=".HomeScreenActivity" />
        <activity
            android:name=".MessageActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HomeScreenUserActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ProfilePage"
            android:parentActivityName=".HomeScreenActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RegisterEntrepreneurActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeScreenActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".CreateClubActivity"
            android:label="Create new club"
            android:parentActivityName=".HomeScreenActivity" />
        <activity android:name=".EventDetailsActivity"></activity>
    </application>

</manifest>